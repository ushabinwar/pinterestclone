<% include ./partials/header.ejs %>
  <div class="w-full min-h-screen bg-zinc-800 px-10 py-10 text-white">
    <div class="cards flex flex-wrap gap-10 ">
        <% posts.forEach(function(elem){ %>
            <div class=" card w-56">
                <div class=" w-full h-44 bg-zinc-700 rounded-lg overflow-hidden">
                    <img class="w-full h-full object-cover" src="/images/uploads/<%= elem.image %>" alt="">
                </div>
                <h5 class="text-sm mt-2"><%= elem.title %></h5>
                <h6 class="text-xs font-semibold opacity-50">@<%= elem.user.username %></h6>
            </div>
            
        <% })%>
      
    </div>
  </div>
<% include ./partials/footer.ejs %>